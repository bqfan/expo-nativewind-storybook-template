{
  "name": "expo-nativewind-template-storybook",
  "description": "An Expo template with NativeWind styling & Storybook (incl. React Native & Web)",
  "version": "1.0.3",
  "author": "Kim Chouard",
  "main": "node_modules/expo/AppEntry.js",
  "license": "MIT",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-generate": "sb-rn-get-stories --config-path .rnstorybook",
    "storybook": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --android"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@gorhom/bottom-sheet": "^5.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-community/slider": "^4.5.5",
    "autoprefixer": "^10.4.17",
    "expo": "^52.0.46",
    "expo-constants": "^17.0.8",
    "expo-status-bar": "^2.0.1",
    "nativewind": "^4.0.1",
    "postcss-loader": "^8.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.76.9",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-web": "~0.19.10",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@chromatic-com/storybook": "^3.2.6",
    "@expo/metro-runtime": "^4.0.1",
    "@storybook/addon-actions": "^8.6.12",
    "@storybook/addon-controls": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-ondevice-actions": "^8.6.2",
    "@storybook/addon-ondevice-backgrounds": "^8.6.2",
    "@storybook/addon-ondevice-controls": "^8.6.2",
    "@storybook/addon-ondevice-notes": "^6.5.7",
    "@storybook/addon-react-native-web": "^0.0.29",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
    "@storybook/react": "^8.6.12",
    "@storybook/react-native": "^8.6.2",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/react": "^18.3.12",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-react-native-web": "^0.19.10",
    "cross-env": "^7.0.3",
    "storybook": "^8.6.12",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "react-docgen-typescript": "2.2.2"
  },
  "overrides": {
    "react-docgen-typescript": "2.2.2"
  },
  "pnpm": {
    "overrides": {
      "react-docgen-typescript": "2.2.2"
    }
  },
  "packageManager": "npm@10.8.3",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
